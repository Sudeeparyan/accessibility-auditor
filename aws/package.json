{
  "name": "accessibility-auditor-aws",
  "version": "1.0.0",
  "description": "Serverless AWS backend for WCAG 2.1 accessibility auditing â€” Lambda, API Gateway, SQS, DynamoDB",
  "scripts": {
    "build": "tsc --noEmit && echo 'TypeScript compilation successful'",
    "typecheck": "tsc --noEmit",
    "deploy": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "remove": "serverless remove --stage dev",
    "offline": "IS_OFFLINE=true serverless offline",
    "logs:api": "serverless logs -f api --stage dev -t",
    "logs:consumer": "serverless logs -f auditConsumer --stage dev -t"
  },
